#!/bin/bash
filebot -rename -r "/storage/media/movies"
filebot -rename -r "/storage/media/tv"

# Fix Permissions
sudo find /storage/media -type d -exec chmod 755 {} \;
sudo find /storage/media -type f -exec chmod 644 {} \;
sudo chown -R /storage/media jerred:jerred

# Remove ACL
setfacl -b -R /storage/media
